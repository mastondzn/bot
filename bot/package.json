{
    "name": "@synopsis/bot",
    "type": "module",
    "version": "0.0.1",
    "private": true,
    "exports": {
        "./rpc": {
            "types": "./dist/rpc.d.ts"
        }
    },
    "scripts": {
        "start": "pnpm with-env tsx src/index.ts",
        "dev": "pnpm start",
        "build": "tsup --entry.rpc src/services/rpc/index.ts --format esm --dts",
        "test": "SKIP_ENV_VALIDATION=true vitest",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "typecheck": "tsc --noEmit",
        "with-env": "dotenv -e ../.env --"
    },
    "dependencies": {
        "@discordjs/collection": "2.1.0",
        "@hono/node-server": "1.11.1",
        "@mastondzn/dank-twitch-irc": "7.1.0",
        "@sentry/node": "8.2.1",
        "@sentry/profiling-node": "8.2.1",
        "@synopsis/db": "workspace:*",
        "@synopsis/env": "workspace:*",
        "@synopsis/scopes": "workspace:*",
        "@twurple/api": "7.1.0",
        "@twurple/auth": "7.1.0",
        "@twurple/eventsub-ws": "7.1.0",
        "chalk": "5.3.0",
        "cron": "3.1.7",
        "eventemitter3": "5.0.1",
        "hono": "4.3.9",
        "ioredis": "5.4.1",
        "is-docker": "3.0.0",
        "p-locate": "6.0.0",
        "pretty-ms": "9.0.0",
        "prom-client": "15.1.2",
        "ramda": "0.30.0",
        "semaphore-async-await": "1.5.1",
        "zod": "3.23.8"
    },
    "devDependencies": {
        "@mastondzn/eslint": "0.9.0",
        "@synopsis/tsconfig": "workspace:*",
        "@types/node": "20.12.12",
        "@types/ramda": "0.30.0",
        "dotenv-cli": "7.4.2",
        "eslint": "^8.57.0",
        "ts-jest": "29.1.4",
        "tsup": "8.0.2",
        "tsx": "4.10.5",
        "typescript": "5.4.5",
        "vitest": "1.6.0"
    }
}
