{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "build": {
            "outputs": ["dist/**", ".next/**"],
            "dependsOn": ["^build"]
        },
        "dev": {
            "cache": false
        },
        "start": {
            "dependsOn": ["^build"],
            "cache": false
        },
        "lint": {},
        "lint:fix": {},
        "typecheck": {
            "dependsOn": ["build"]
        },
        "db:migrate": {
            "outputs": ["migrations/**"],
            "dependsOn": ["^build"]
        },
        "db:check": {
            "cache": false
        },
        "db:generate": {
            "cache": false
        }
    },
    "globalDependencies": ["./packages/eslint-config/index.cjs", "./packages/tsconfig"],
    "globalEnv": []
}
